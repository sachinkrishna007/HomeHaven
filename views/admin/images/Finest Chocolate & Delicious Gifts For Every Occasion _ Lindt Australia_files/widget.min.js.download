define(['jquery','Magento_PageBuilder/js/events','matchMedia','jquery-ui-modules/tabs'],function($,events,mediaCheck){'use strict';return function(config,element){var $element=$(element);if($element.is('.pagebuilder-tabs')){return;}
$.ui.tabs({active:$element.data('activeTab')||0,create:function(){var borderWidth=parseInt($element.find('.tabs-content').css('borderWidth').toString(),10);$element.find('.tabs-navigation').css('marginBottom',-borderWidth);$element.find('.tabs-navigation li:not(:first-child)').css('marginLeft',-borderWidth);},activate:function(){events.trigger('contentType:redrawAfter',{element:element});var sliders=$(this).find('.js-products-carousel .product-items');if(sliders.length){sliders.each(function(i,el){var slider=$(el);if(slider.hasClass('slick-initialized')){slider.slick('setPosition');}});}}},element);function adjustTabsWidthMobile(element){var tabsNavigation=$(element).find('.tabs-navigation');var tabsNavigationItems=tabsNavigation.find('.tab-header');var totalWidth=0;tabsNavigationItems.each(function(i,el){totalWidth+=$(el).outerWidth();});tabsNavigation.width(totalWidth+15);}
function adjustTabsWidthDesktop(element){var tabsNavigation=$(element).find('.tabs-navigation');if(tabsNavigation[0].style.width.length){tabsNavigation[0].style.width='';}}
