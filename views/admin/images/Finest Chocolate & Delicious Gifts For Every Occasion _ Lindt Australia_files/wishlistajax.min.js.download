define(['jquery','uiComponent','Magento_Customer/js/customer-data','domReady!'],function($,Component,customerData){'use strict';return Component.extend({initialize:function(config,element){var self=this;this._super();this.wishlist=customerData.get('wishlist');this.elem=element;this.wishlist.subscribe(function(newWishlist){self.addWishlistActiveClass();});self.addWishlistActiveClass();},addWishlistActiveClass:function(){var items=this.wishlist().items;var i;var wislistAddedProductId;if(typeof items==='undefined'||!items.length)return;for(i=0;i<items.length;i++){wislistAddedProductId=items[i].product_id;$(this.elem).find('.product-id-'+wislistAddedProductId).addClass('product-item-info--wishlist-added');}}});});